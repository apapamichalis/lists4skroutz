<div class="container">
  <div class="panel panel-primary">
  <div class="panel-heading">
    <h3>Users</h3>
    </div>
    <div class="panel-body">
      <table class="table table-hover">
        <tbody>
          <% @users.each do |user| %>
            
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
